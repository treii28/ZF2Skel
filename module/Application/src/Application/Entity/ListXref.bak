<?php

namespace Application\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ListXref
 *
 * @ORM\Table(name="ListXref", uniqueConstraints={@ORM\UniqueConstraint(name="ListMember_idx", columns={"ListId", "MemberId"})})
 * @ORM\Entity
 */
class ListXref
{
    /**
     * @var integer
     *
     * @ORM\Column(name="ListId", type="integer", nullable=false)
     */
    private $ListId;

    /**
     * @var integer
     *
     * @ORM\Column(name="SubListId", type="integer", nullable=true)
     */
    private $SubListId;

    /**
     * @var integer
     *
     * @ORM\Column(name="MemberId", type="integer", nullable=false)
     */
    private $MemberId;

    /**
     * @var string
     *
     * @ORM\Column(name="EntityName", type="string", length=64, nullable=true)
     */
    private $EntityName;

    /**
     * @var integer
     *
     * @ORM\Column(name="listXrefId", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $listXrefId;

    /**
     * @var \Application\Entity\Lists
     *
     * @ORM\ManyToOne(targetEntity="Application\Entity\Lists")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="ListId", referencedColumnName="ListId")
     * })
     */
    private $list;

    /**
     * @var \Application\Entity\Lists
     *
     * @ORM\ManyToOne(targetEntity="Application\Entity\Lists")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="SubListId", referencedColumnName="ListId")
     * })
     */
    private $sublist;


    /**
     * Set ListId
     *
     * @param integer $listId
     * @return ListXref
     */
    public function setListId($listId)
    {
        $this->ListId = $listId;

        return $this;
    }

    /**
     * Get ListId
     *
     * @return integer 
     */
    public function getListId()
    {
        return $this->ListId;
    }

    /**
     * Set SubListId
     *
     * @param integer $subListId
     * @return ListXref
     */
    public function setSubListId($subListId)
    {
        $this->SubListId = $subListId;

        return $this;
    }

    /**
     * Get SubListId
     *
     * @return integer 
     */
    public function getSubListId()
    {
        return $this->SubListId;
    }

    /**
     * Set MemberId
     *
     * @param integer $memberId
     * @return ListXref
     */
    public function setMemberId($memberId)
    {
        $this->MemberId = $memberId;

        return $this;
    }

    /**
     * Get MemberId
     *
     * @return integer 
     */
    public function getMemberId()
    {
        return $this->MemberId;
    }

    /**
     * Set EntityName
     *
     * @param string $entityName
     * @return ListXref
     */
    public function setEntityName($entityName)
    {
        $this->EntityName = $entityName;

        return $this;
    }

    /**
     * Get EntityName
     *
     * @return string 
     */
    public function getEntityName()
    {
        return $this->EntityName;
    }

    /**
     * Get listXrefId
     *
     * @return integer 
     */
    public function getListXrefId()
    {
        return $this->listXrefId;
    }

    /**
     * Set list
     *
     * @param \Application\Entity\Lists $list
     * @return ListXref
     */
    public function setList(\Application\Entity\Lists $list = null)
    {
        $this->list = $list;

        return $this;
    }

    /**
     * Get list
     *
     * @return \Application\Entity\Lists 
     */
    public function getList()
    {
        return $this->list;
    }

    /**
     * Set sublist
     *
     * @param \Application\Entity\Lists $sublist
     * @return ListXref
     */
    public function setSublist(\Application\Entity\Lists $sublist = null)
    {
        $this->sublist = $sublist;

        return $this;
    }

    /**
     * Get sublist
     *
     * @return \Application\Entity\Lists 
     */
    public function getSublist()
    {
        return $this->sublist;
    }
}
